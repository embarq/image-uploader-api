{
  "extends": "ts-node/node16/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "resolveJsonModule": true,
    "removeComments": true,
    "noEmit": true,
    "outDir": "./dist",
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "node"
    ],
    "sourceMap": true
  },
  "ts-node": {
    "files": true,
  },
  "include": ["./src"],
  "exclude": ["./dist"]
}
