{
  "name": "image-uploader",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "node dist node --nouse-idle-notification --expose-gc --max-old-space-size=512 --report-uncaught-exception --report-on-signal --report-on-fatalerror",
    "build": "npx tsc --noemit false",
    "dev": "NODE_ENV=development nodemon --watch './src/**/*.ts' -e ts --exec 'ts-node' src"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.165.0",
    "@aws-sdk/lib-storage": "^3.165.0",
    "dotenv": "^16.0.2",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.4",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^18.7.15",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1"
  }
}
